  <script>
    const form = document.querySelector("#email_form")
    
    function sendEmail(e) {
      e.preventDefault();

      const radioBox = document.getElementsByName('personal');
      
      const title = document.getElementsByName('form_title')
      const name = document.getElementsByName('form_name')
      const company = document.getElementsByName('form_company')
      const phone = document.getElementsByName('form_phone')
      const email = document.getElementsByName('form_e-mail')
      const question = document.getElementsByName('form_questioin')
      
      if (title[0].value.length === 0 || name[0].value.length === 0 || company[0].value.length === 0 || phone[0].value.length === 0 || email[0].value.length === 0) {
        alert("기본정보를 입력하세요.")
      } else if (radioBox[0].checked === false) {
        alert("개인정보 처리방침에 동의해주세요.")
      } else {
        Email.send({
          SecureToken: "9a8c5133-2ae7-4a7a-bae4-1c3069a9fa51",
          To: 'info@eco-planet.io',
          From: 'info@eco-planet.io',
          Subject: title[0].value,
          Body: `[Name: ${name[0].value}][Company: ${company[0].value}][Phone: ${phone[0].value}][Email: ${email[0].value}] ${question[0].value}`
        }).then(
          message => alert(message, title.value)
        );
      }
    }

    form.addEventListener("submit", sendEmail)
  </script>

