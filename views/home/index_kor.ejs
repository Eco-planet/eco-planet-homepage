        alert("개인정보 처리방침에 동의해주세요.")
      } else {
        Email.send({
          SecureToken: "9a8c5133-2ae7-4a7a-bae4-1c3069a9fa51",
          To: 'info@eco-planet.io',
          From: 'info@eco-planet.io',
          Subject: title[0].value,
          Body: `[Name: ${name[0].value}][Company: ${company[0].value}][Phone: ${phone[0].value}][Email: ${email[0].value}] ${question[0].value}`
        }).then(
          message => alert(message, title.value)
        );
      }
    }

    form.addEventListener("submit", sendEmail)
